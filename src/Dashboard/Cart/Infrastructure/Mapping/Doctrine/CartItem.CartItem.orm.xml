<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping">
    <entity name="App\Dashboard\Cart\Domain\Aggregate\CartItem\CartItem">
        <embedded name="id" class="App\Dashboard\Cart\Domain\Aggregate\CartItem\CartItemId" use-column-prefix="false" />
        <embedded name="name" class="App\Dashboard\Cart\Domain\Aggregate\CartItem\CartItemName" use-column-prefix="false" />
        <embedded name="description" class="App\Dashboard\Cart\Domain\Aggregate\CartItem\CartItemDescription" use-column-prefix="false" />
        <embedded name="price" class="App\Dashboard\Cart\Domain\Aggregate\CartItem\CartItemPrice" use-column-prefix="false" />
        <field name="active" column="active" type="boolean" />
    </entity>
</doctrine-mapping>